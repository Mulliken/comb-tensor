import numpy as np

class test:
    @property
    def h(self):
        return self._h

    @h.setter
    def h(self, m):
        print("Change")
        self._h = m

    def __init__(self):
        self._h = [1, 2]

def eye(*args, d=None):
    if not args and d is None:
        return None
    elif not args and d is not None:
        return np.eye(d)
    else:
        return np.eye(*args)

if __name__ == "__main__":
    a = test()
    a.h[1] = 9
    a.h = a.h
    print(a.h)
    a  = [[1],[2],2]
    print([eye(*d) for d in a])
